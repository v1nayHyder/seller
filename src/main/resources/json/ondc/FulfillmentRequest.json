{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Seller can add/update the fulfillment details",
  "type": "object",
  "properties": {
    "id": {
      "type": "id"
    },
    "sellerId": {
      "type": "integer"
    },
    "type": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "isTracking": {
      "type": "boolean"
    },
    "ondcOrgCategory": {
      "type": "string"
    },
    "ondcOrgProviderName": {
      "type": "string"
    },
    "ondcOrgTat": {
      "type": "string"
    }

  },
  "required": ["type","sellerId","isActive","phone"]
}